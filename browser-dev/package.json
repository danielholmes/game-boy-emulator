    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
