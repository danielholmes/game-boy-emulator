{
  "name": "@gebby/browser-dev",
  "version": "0.0.1",
  "description": "Browser based ui for playing the game boy with development outputs/tools",
  "license": "MIT",
  "scripts": {
    "ts": "tsc -p tsconfig.json",
    "lint": "eslint --fix ./src --ext .ts,.tsx",
    "lint:check": "eslint ./src  --ext .ts,.tsx",
    "start": "react-scripts start",
    "build": "concurrently \"yarn lint:check\" \"yarn test\" && react-scripts build",
    "test": "react-scripts test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.123",
    "concurrently": "^4.1.0",
    "eslint": "^6.1.0",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "react-scripts": "^3.1.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "@gebby/core": "^0.0.1",
    "@material-ui/core": "^4.3.2",
    "lodash": "^4.17.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
